   "source": [
    "### Defanging an IP Address"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "id": "b4e4f1fb",
   "metadata": {},
   "outputs": [],
   "source": [
    "class Solution:\n",
    "    def defangIPaddr(self, address: str) -> str:\n",
    "        return address.replace(\".\",\"[.]\",3)\n",
    "          "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "id": "b8cd26f3",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "127[.]0[.]0[.]1\n"
     ]
    }
   ],
   "source": [
    "S = Solution()\n",
    "\n",
    "print(S.defangIPaddr(\"127.0.0.1\"))"
   ]
  }
 ],
